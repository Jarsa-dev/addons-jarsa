<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="id" model="ir.ui.view">
		<field name="name">luch order</field>
		<field name="model">lunch.order</field>
		<field name="inherit_id" ref="lunch.lunch_order_view_form"/>
		<field name="arch" type="xml">
			<field name="order_line_ids" position="replace">
				<field name="order_line_ids">
					<tree editable="bottom" string="List">
						<field name="product_id"/>
						<field name="note"/>
						<field name="quantity"/>
						<field name="price"/>
						<field name="total"/>
						<field invisible="1" name="supplier"/>
						<field invisible="1" name="state"/>
					</tree>
				</field>
			</field>
		</field>
	</record>
</odoo>